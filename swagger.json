{
  "openapi": "3.0.3",
  "info": {
    "title": "Swagger AppWeave - V2",
    "description": "Here is the Swagger page of the RestAPI AppWeave, listing all the routes that can be used for RestAPI calls. The GitHub [link](https://github.com/anthonylegrand/AppWeaveV2) is also available for further documentation.",
    "contact": {
      "email": "contact@appweave.online"
    },
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "http://localhost:81"
    },
    {
      "url": "http://api.appweave.online"
    }
  ],
  "tags": [
    {
      "name": "kuma",
      "description": "Call a test route to check api availability"
    },
    {
      "name": "user",
      "description": "Interact with the user authenticated by the authentication token"
    },
    {
      "name": "project",
      "description": "Interact with the projects of the user authenticated by the authentication token"
    }
  ],
  "paths": {
    "/kuma/ping": {
      "get": {
        "tags": ["kuma"],
        "summary": "Test server availability",
        "description": "Can be called without authentication",
        "operationId": "pingService",
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/kuma"
                }
              }
            }
          }
        }
      }
    },
    "/user/profil": {
      "get": {
        "tags": ["user"],
        "summary": "Retrieve profile information",
        "description": "Must be authenticated to be called",
        "operationId": "authProfil",
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/user"
                }
              }
            }
          },
          "401": {
            "description": "Unauthenticated request",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/noAuth"
                }
              }
            }
          }
        },
        "security": [{ "bearerAuth": [] }]
      }
    },
    "/project": {
      "post": {
        "tags": ["project"],
        "summary": "Create a new project for the user",
        "description": "Must be authenticated to be called",
        "operationId": "projectCreation",
        "requestBody": {
          "description": "Project JSON body",
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "displayName": {
                    "type": "string",
                    "example": "My Super Project",
                    "minLength": 1,
                    "maxLength": 50
                  }
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/project"
                }
              }
            }
          },
          "401": {
            "description": "Unauthenticated request",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/noAuth"
                }
              }
            }
          },
          "500": {
            "description": "Error message"
          }
        },
        "security": [{ "bearerAuth": [] }]
      }
    },
    "/project/list": {
      "get": {
        "tags": ["project"],
        "summary": "Retrieve a list of all the user's projects",
        "description": "Must be authenticated to be called",
        "operationId": "projectList",
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/project_light"
                  }
                }
              }
            }
          },
          "401": {
            "description": "Unauthenticated request",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/noAuth"
                }
              }
            }
          },
          "500": {
            "description": "Error message"
          }
        },
        "security": [{ "bearerAuth": [] }]
      }
    },
    "/project/{projectUuid}": {
      "get": {
        "tags": ["project"],
        "summary": "Get a project for the user with uuid",
        "description": "Must be authenticated to be called",
        "operationId": "projectGet",
        "parameters": [
          {
            "name": "projectUuid",
            "in": "path",
            "description": "The uuid of a project already created",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/project_light"
                }
              }
            }
          },
          "400": {
            "description": "Project uuid not found"
          },
          "401": {
            "description": "Unauthenticated request",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/noAuth"
                }
              }
            }
          },
          "500": {
            "description": "Error message"
          }
        },
        "security": [{ "bearerAuth": [] }]
      },
      "put": {
        "tags": ["project"],
        "summary": "Update a project for the user with uuid",
        "description": "Must be authenticated to be called",
        "operationId": "projectUpdate",
        "requestBody": {
          "description": "Project JSON body",
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "displayName": {
                    "type": "string",
                    "example": "My Super Project",
                    "minLength": 1,
                    "maxLength": 50
                  }
                }
              }
            }
          }
        },
        "parameters": [
          {
            "name": "projectUuid",
            "in": "path",
            "description": "The uuid of a project already created",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/default_result"
                }
              }
            }
          },
          "400": {
            "description": "Project uuid not found"
          },
          "401": {
            "description": "Unauthenticated request",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/noAuth"
                }
              }
            }
          },
          "500": {
            "description": "Error message"
          }
        },
        "security": [{ "bearerAuth": [] }]
      },
      "delete": {
        "tags": ["project"],
        "summary": "Delete a project for the user with uuid",
        "description": "Must be authenticated to be called",
        "operationId": "projectDelete",
        "parameters": [
          {
            "name": "projectUuid",
            "in": "path",
            "description": "The uuid of a project already created",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/default_result"
                }
              }
            }
          },
          "400": {
            "description": "Project uuid not found"
          },
          "401": {
            "description": "Unauthenticated request",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/noAuth"
                }
              }
            }
          },
          "500": {
            "description": "Error message"
          }
        },
        "security": [{ "bearerAuth": [] }]
      }
    },
    "/project/resetPrivate/{projectUuid}": {
      "put": {
        "tags": ["project"],
        "summary": "Regenerate a project's private key for the user with uuid",
        "description": "Must be authenticated to be called",
        "operationId": "projectResetPrivateKey",
        "parameters": [
          {
            "name": "projectUuid",
            "in": "path",
            "description": "The uuid of a project already created",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/project"
                  }
                }
              }
            }
          },
          "400": {
            "description": "Any projects found"
          },
          "401": {
            "description": "Unauthenticated request",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/noAuth"
                }
              }
            }
          },
          "500": {
            "description": "Error message"
          }
        },
        "security": [{ "bearerAuth": [] }]
      }
    }
  },
  "components": {
    "schemas": {
      "default_result": {
        "type": "object",
        "properties": {
          "sucess": {
            "type": "boolean"
          }
        }
      },
      "noAuth": {
        "type": "object",
        "properties": {
          "sucess": {
            "type": "boolean",
            "exemple": false
          },
          "error": {
            "type": "string",
            "exemple": "Token missing. Authentication required."
          }
        }
      },
      "user": {
        "type": "object",
        "properties": {
          "uid": {
            "type": "string",
            "example": "QkZApItXjJVKHUVMy9MQggjCYG23"
          },
          "name": {
            "type": "string",
            "example": "John James"
          },
          "email": {
            "type": "string",
            "example": "john@email.com"
          },
          "email_verified": {
            "type": "boolean",
            "example": true
          },
          "anonymous": {
            "type": "boolean",
            "example": false
          },
          "createdAt": {
            "type": "string",
            "format": "date-time"
          }
        }
      },
      "project_light": {
        "type": "object",
        "properties": {
          "uuid": {
            "type": "string",
            "example": "34424197-5a7f-4fff-b0df-a8ef5cac2c90"
          },
          "ownerUid": {
            "type": "string",
            "example": "QkZApItXjJVKHUVMy9MQggjCYG23"
          },
          "displayName": {
            "type": "string",
            "example": "My Super Project"
          },
          "publicKey": {
            "type": "string",
            "example": "Dld8SH",
            "minLength": 6,
            "maxLength": 6
          },
          "updatedAt": {
            "type": "string",
            "format": "date-time"
          }
        }
      },
      "project": {
        "type": "object",
        "properties": {
          "uuid": {
            "type": "string",
            "example": "34424197-5a7f-4fff-b0df-a8ef5cac2c90"
          },
          "ownerUid": {
            "type": "string",
            "example": "QkZApItXjJVKHUVMy9MQggjCYG23"
          },
          "displayName": {
            "type": "string",
            "example": "My Super Project"
          },
          "publicKey": {
            "type": "string",
            "example": "Dld8SH",
            "minLength": 6,
            "maxLength": 6
          },
          "privateKey": {
            "type": "string",
            "example": "XwPp9xazJ0ku5CZnlmgAx2",
            "minLength": 22,
            "maxLength": 22
          },
          "createdAt": {
            "type": "string",
            "format": "date-time"
          },
          "updatedAt": {
            "type": "string",
            "format": "date-time"
          }
        }
      },
      "kuma": {
        "type": "object",
        "properties": {
          "ping": {
            "type": "string",
            "example": "pong"
          }
        }
      }
    },
    "securitySchemes": {
      "bearerAuth": {
        "type": "http",
        "description": "JWT Authorization header using the Bearer scheme.",
        "scheme": "bearer",
        "bearerFormat": "JWT"
      }
    }
  }
}
